{
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.settings.editor": "json",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.indent": 14,
    "workbench.list.openMode": "doubleClick",
    // === Editor ===
    "editor.fontFamily": "Menlo,DejaVu Sans Mono, Source Code Pro, Inconsolata, Monaco, 'Courier New', monospace",
    "editor.fontSize": 12,
    "editor.wordWrap": "on",
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "solid",
    "editor.renderWhitespace": "all",
    "editor.guides.bracketPairs": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.inlayHints.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.accessibilitySupport": "off",
    "editor.minimap.autohide": true,
    "editor.minimap.renderCharacters": false,
    "editor.folding": true,
    "editor.glyphMargin": true,
    "editor.rulers": [
        120
    ],
    "editor.stickyScroll.enabled": true,
    // === Terminal ===
    "terminal.integrated.fontFamily": "Menlo,DejaVu Sans Mono, Source Code Pro, Inconsolata, Monaco, 'Courier New', monospace",
    "terminal.integrated.fontSize": 12,
    // "terminal.integrated.inheritEnv": false,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.persistentSessionScrollback": 100000000,
    "terminal.integrated.lineHeight": 1.5,
    // === Diff Editor ===
    "diffEditor.wordWrap": "inherit",
    // === Git Graph ===
    "git-graph.repository.fetchAndPrune": true,
    // // === Explorer ===
    "explorer.sortOrder": "foldersNestsFiles",
    "explorer.confirmDragAndDrop": false,
    // === Notebook ===
    "notebook.insertToolbarLocation": "notebookToolbar",
    "notebook.output.textLineLimit": 40,
    "notebook.output.lineHeight": 18,
    "notebook.stickyScroll.enabled": true,
    // === Python ===
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    // Local repo for development
    "python.analysis.extraPaths": [
        "/Users/luisaceituno/projects/"
    ],
    "python.analysis.autoFormatStrings": true,
    // "python.analysis.autoImportCompletions": true, // Very error-prone!
    "python.analysis.typeCheckingMode": "standard",
    // Python venv
    // "python.defaultInterpreterPath": "./.venv",
    // "python.terminal.activateEnvironment": true,
    "python.analysis.exclude": [
        "**/venv/**",
        "**/.venv/**",
        "**/.vscode/**",
        "**/.git/**",
        "**/__pycache__/**",
        "**/.mypy_cache/**",
        "**/.pytest_cache/**",
        "**/node_modules/**",
        "**/dist/**",
        "**/build/**",
        "**/site-packages"
    ],
    // === Markdown ===
    "[markdown]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    },
    // "editor.codeActionsOnSave": {
    //     "source.fixAll.markdownlint": true
    // },
    "markdownlint.config": {
        "MD024": false,
        "ignore": [
            "**/CODEOWNERS",
            "**/.github/CODEOWNERS" // Add this line if your CODEOWNERS file is in the .github directory
        ]
    },
    // "markdownlint.focusMode": 2,
    "markdownlint.lintWorkspaceGlobs": [
        "**/*.md",
        "!**/CODEOWNERS",
        "!**/.github/CODEOWNERS" // Add this line if your CODEOWNERS file is in the .github directory
    ],
    // "github.copilot.enable": {
    //     "*": true,
    //     // "yaml": false,
    //     "plaintext": true,
    //     "markdown": true
    // },
    // === JavaScript ===
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // === JSON ===
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    // === Updates and Telemetry ===
    "update.mode": "manual",
    "telemetry.telemetryLevel": "off",
    "redhat.telemetry.enabled": false,
    // === Visual Customizations ===
    "workbench.fontAliasing": "antialiased",
    "workbench.colorCustomizations": {
        "tab.activeBorderTop": "#ff00ee",
        // "tab.activeBorder": "#ff00ee",
        "notebook.focusedCellBackground": "#0099ff30",
        "editor.lineHighlightBackground": "#008cff21",
        "editor.lineHighlightBorder": "#9fced11f",
        "tree.indentGuidesStroke": "#EBEBEB",
        "editor.findMatchBackground": "#8ffa876b",
        "editor.findMatchHighlightBackground": "#ff7dbe75",
        "sqltools.currentQueryBg": "#7b798a03",
        // "sqltools.currentQueryOutline": "#b8c1e5"
    },
    "workbench.tree.enableStickyScroll": true,
    "editor.tokenColorCustomizations": {
        "comments": "#aaaaaa",
        "textMateRules": [
            {
                "scope": "heading.1.markdown entity.name.section.markdown, heading.1.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#e86f4f"
                }
            },
            {
                "scope": "heading.2.markdown entity.name.section.markdown, heading.2.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#f4a361"
                }
            },
            {
                "scope": "heading.3.markdown entity.name.section.markdown, heading.3.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#eac56a"
                }
            },
            {
                "scope": "heading.4.markdown entity.name.section.markdown, heading.4.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#269f91"
                }
            },
            {
                "scope": "heading.5.markdown entity.name.section.markdown, heading.5.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#5d99b0"
                }
            }
        ]
    },
    // === GitHub ===
    "githubPullRequests.remotes": [
        "origin",
        "upstream"
    ],
    // === Miscellaneous ===
    "jupyter.askForKernelRestart": false,
    "git-graph.commitDetailsView.location": "Docked to Bottom",
    "git-graph.date.format": "ISO Date & Time",
    "git-graph.graph.style": "angular",
    "git-graph.maxDepthOfRepoSearch": 1,
    "markdown-preview-github-styles.darkTheme": "dark_high_contrast",
    "window.openFilesInNewWindow": "on",
    "window.zoomLevel": -1,
    "markdown-pdf.styles": [
        "/Users/luisaceituno/.config/markdown-pdf/markdown-pdf.css"
        // , "/Users/luisaceituno/.config/markdown-pdf/markdown-here.css"
    ],
    "notebook.diff.ignoreMetadata": true,
    "git.openRepositoryInParentFolders": "always",
    "editor.minimap.enabled": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "diffEditor.experimental.showMoves": true,
    "git.autofetch": true,
    "githubPullRequests.pushBranch": "always",
    "githubPullRequests.pullBranch": "never",
    "workbench.productIconTheme": "fluent-icons",
    "window.commandCenter": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.sideBar.location": "right",
    "workbench.activityBar.location": "top",
    "codesnap.containerPadding": "0.5em",
    "codesnap.showWindowControls": false,
    "codesnap.showWindowTitle": true,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "scm.workingSets.enabled": true,
    "workbench.colorTheme": "Monokai",
    "github.copilot.editor.enableAutoCompletions": true,
    // Git
    "git.autoStash": true,
    "git.autoRepositoryDetection": "openEditors",
    "githubPullRequests.fileListLayout": "tree",
    "scm.workingSets.enabled": false,
    "scm.workingSets.default": "empty",
    "workbench.editor.alwaysShowEditorActions": true,
    "indentRainbow.excludedLanguages": [
        "sql",
        "jinja-sql"
    ],
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    // dbt
    "files.associations": {
        "*.sql": "jinja-sql",
        "*.yaml": "jinja-yaml",
        "*.yml": "jinja-yaml",
    },
    // "[jinja-sql]": {
    //     "editor.defaultFormatter": "henriblancke.vscode-dbt-formatter",
    //     "editor.formatOnSave": true
    // },
    "[jinja-yaml]": {
        // "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.formatOnSave": true
    },
    // SQL formatting
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".*\\.sql(\\.jinja)?",
                "isAsync": true,
                "cmd": "sqlfmt ${file}"
            }
        ]
    },
    "dbt.allowListFolders": [
        "/Users/luisaceituno/projects/hf_repo/tardis-community/pipelines/intl-scm-analytics/scm-analytics-engineers/dbt/"
    ],
    "yaml.schemas": {
        "file:///Users/luisaceituno/.vscode/extensions/docsmsft.docs-yaml-1.0.5/dist/toc.schema.json": "/toc\\.yml/i"
    },
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "is:open assignee:${user} repo:${owner}/${repository}",
            "groupBy": [
                "milestone"
            ]
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Recent Issues",
            "query": "state:open repo:${owner}/${repository} sort:updated-desc"
        }
    ],
    "github.copilot.enable": {
        "markdown": true
    },
    "makefile.configureOnOpen": true,
    "sort-json.settings.sortLevel": 1,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "yaml.schemas": {
        "file:///Users/luisaceituno/.vscode/extensions/docsmsft.docs-yaml-1.0.5/dist/toc.schema.json": "/toc\\.yml/i"
    },
    "editor.lineNumbers": "relative",
    "cSpell.enableFiletypes": [
        "!jsonc",
        "!python"
    ],
    "cSpell.userWords": [
        "OTIF",
        "otifiq"
    ],
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "aws.telemetry": false,
    // "[sql]": {
    //     "editor.defaultFormatter": "innoverio.vscode-dbt-power-user"
    // },
    "github.copilot.chat.edits.enabled": false,
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "githubPullRequests.assignCreated": "${user}",
    "explorer.compactFolders": false,
    // Nov 2024
    "git.blame.statusBarItem.enabled": true,
    "notebook.cellExecutionTimeVerbosity": "verbose",
    "[python]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit",
            "source.organizeImports": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "notebook.formatOnSave.enabled": true,
    "notebook.codeActionsOnSave": {
        "notebook.source.fixAll": "explicit",
        "notebook.source.organizeImports": "explicit"
    },
    "ruff.nativeServer": "on", 
    // "ruff.configuration": "/Users/luisaceituno/.config/ruff/ruff.toml",
    // "ruff.format.args": [
    //     "\"args\": [\"--config=/Users/luisaceituno/.config/ruff/ruff.toml\"]"
