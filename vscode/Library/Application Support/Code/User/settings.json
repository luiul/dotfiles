{
    // === Workbench Settings ===
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.settings.editor": "json",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.indent": 14,
    "workbench.list.openMode": "doubleClick",
    "workbench.fontAliasing": "antialiased",
    "workbench.colorCustomizations": {
        "tab.activeBorderTop": "#ff00ee",
        "notebook.focusedCellBackground": "#0099ff30",
        "editor.lineHighlightBackground": "#008cff28",
        "editor.lineHighlightBorder": "#9fced11f",
        // "tree.indentGuidesStroke": "#EBEBEB",
        "editor.findMatchBackground": "#8ffa876b",
        "editor.findMatchHighlightBackground": "#ff7dbe75",
        "sqltools.currentQueryBg": "#7b798a03",
        "tree.indentGuidesStroke": "#51bdd8",
        "tree.inactiveIndentGuidesStroke": "#232a35",
        "editorLineNumber.foreground": "#3a3f4b",
        "editorLineNumber.activeForeground": "#6b717d"
    },
    "workbench.tree.enableStickyScroll": true,
    "workbench.sideBar.location": "right",
    "workbench.activityBar.location": "top",
    "workbench.editor.alwaysShowEditorActions": true,
    // === Editor Settings ===
    "editor.fontFamily": "Menlo, DejaVu Sans Mono, Source Code Pro, Inconsolata, Monaco, Courier New, monospace",
    "editor.fontSize": 12,
    "editor.wordWrap": "on",
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "solid",
    "editor.renderWhitespace": "all",
    "editor.guides.bracketPairs": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.inlineSuggest.enabled": true,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.minimap.autohide": "mouseover",
    "editor.minimap.renderCharacters": false,
    "editor.folding": true,
    "editor.glyphMargin": true,
    "editor.rulers": [
        120
    ],
    "editor.stickyScroll.enabled": true,
    "editor.tokenColorCustomizations": {
        "comments": "#aaaaaa",
        "textMateRules": [
            {
                "scope": "heading.1.markdown entity.name.section.markdown, heading.1.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#e86f4f"
                }
            },
            {
                "scope": "heading.2.markdown entity.name.section.markdown, heading.2.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#f4a361"
                }
            },
            {
                "scope": "heading.3.markdown entity.name.section.markdown, heading.3.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#eac56a"
                }
            },
            {
                "scope": "heading.4.markdown entity.name.section.markdown, heading.4.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#269f91"
                }
            },
            {
                "scope": "heading.5.markdown entity.name.section.markdown, heading.5.markdown punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#5d99b0"
                }
            }
        ]
    },
    "editor.lineNumbers": "relative",
    // "editor.minimap.enabled": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.formatOnSave": true,
    // === Terminal Settings ===
    "terminal.integrated.fontFamily": "Menlo,DejaVu Sans Mono, Source Code Pro, Inconsolata, Monaco, Courier New, monospace",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.persistentSessionScrollback": 1000,
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.tabs.hideCondition": "never",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "none",
    "terminal.integrated.cursorWidth": 2,
    // === Diff Editor Settings ===
    "diffEditor.wordWrap": "inherit",
    "diffEditor.experimental.showMoves": true,
    // === Explorer Settings ===
    "explorer.sortOrder": "foldersNestsFiles",
    "explorer.confirmDragAndDrop": false,
    "explorer.compactFolders": false,
    // === Notebook Settings ===
    "notebook.insertToolbarLocation": "notebookToolbar",
    "notebook.output.textLineLimit": 40,
    "notebook.output.lineHeight": 18,
    "notebook.stickyScroll.enabled": true,
    "notebook.diff.ignoreMetadata": true,
    "notebook.cellExecutionTimeVerbosity": "verbose",
    // === Git Settings ===
    "git-graph.repository.fetchAndPrune": true,
    "git-graph.commitDetailsView.location": "Docked to Bottom",
    "git-graph.date.format": "ISO Date & Time",
    "git-graph.graph.style": "angular",
    "git-graph.maxDepthOfRepoSearch": 1,
    "git.autoStash": true,
    "git.autoRepositoryDetection": "openEditors",
    "git.openRepositoryInParentFolders": "always",
    // "git.blame.statusBarItem.enabled": true,
    "git.autofetch": true,
    // === GitHub Settings ===
    "githubPullRequests.remotes": [
        "origin",
        "upstream"
    ],
    "githubPullRequests.pushBranch": "always",
    "githubPullRequests.pullBranch": "never",
    "githubPullRequests.fileListLayout": "tree",
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "is:open assignee:${user} repo:${owner}/${repository}",
            "groupBy": [
                "milestone"
            ]
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Recent Issues",
            "query": "state:open repo:${owner}/${repository} sort:updated-desc"
        }
    ],
    // === Language-Specific Settings ===
    "[markdown]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "diffEditor.ignoreTrimWhitespace": false
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    // Python formatter
    "[python]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    //
    // Ruff
    //
    "ruff.path": [
        "/Users/luisaceituno/.local/bin/ruff"
    ],
    "ruff.importStrategy": "fromEnvironment",
    // "ruff.interpreter": [
    //     "${workspaceFolder}/.venv/bin/python"
    // ],
    "ruff.configuration": "/Users/luisaceituno/.config/ruff/ruff.toml",
    //
    // Ty
    //
    "ty.path": [
        "/Users/luisaceituno/.local/bin/ty"
    ],
    "ty.importStrategy": "fromEnvironment",
    "ty.interpreter": [
        "${workspaceFolder}/.venv/bin/python"
    ],
    // Jupyter Notebooks formatter
    "notebook.formatOnSave.enabled": true,
    "notebook.codeActionsOnSave": {
        "notebook.source.fixAll": "explicit"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.formatOnSave": true
    },
    "[jinja-yaml]": {
        "editor.formatOnSave": true
    },
    // === Python Analysis ===
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    // "python.analysis.extraPaths": [
    //     "/Users/luisaceituno/projects/",
    //     "${workspaceFolder}/src"
    // ],
    "python.analysis.autoFormatStrings": true,
    "python.analysis.typeCheckingMode": "standard",
    "python.analysis.exclude": [
        "**/venv/**",
        "**/.venv/**",
        "**/.vscode/**",
        "**/.git/**",
        "**/__pycache__/**",
        "**/.mypy_cache/**",
        "**/.pytest_cache/**",
        "**/node_modules/**",
        "**/dist/**",
        "**/build/**",
        "**/site-packages"
    ],
    // === Python Virtual Environment ===
    "python.envFile": "${workspaceFolder}/.env",
    // "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "python.terminal.activateEnvironment": false,
    "python.terminal.activateEnvInCurrentTerminal": false,
    // === Python Testing ===
    "python.testing.pytestEnabled": true,
    "python.testing.cwd": "${workspaceFolder}/tests",
    "python.testing.pytestPath": "${workspaceFolder}/.venv/bin/pytest",
    // === Miscellaneous ===
    "update.mode": "manual",
    "telemetry.telemetryLevel": "off",
    "redhat.telemetry.enabled": false,
    "window.zoomLevel": -1,
    "window.openFilesInNewWindow": "on",
    // === Pylint ===
    "files.exclude": {
        "**/__pycache__": true
    },
    "files.autoSave": "onWindowChange",
    "debug.toolBarLocation": "docked",
    "yaml.schemas": {
        "file:///Users/luisaceituno/.vscode/extensions/docsmsft.docs-yaml-1.0.5/dist/toc.schema.json": "/toc\\.yml/i"
    },
    "workbench.colorTheme": "Monokai",
    "python.languageServer": "Pylance",
    "editor.inlayHints.enabled": "offUnlessPressed",
    "jupyter.askForKernelRestart": false,
    // "markdown-preview-github-styles.darkTheme": "dark_high_contrast",
    // "markdown-pdf.styles": [
    //     "/Users/luisaceituno/.config/markdown-pdf/markdown-pdf.css"
    // ],
    // "scm.workingSets.enabled": false,
    // "scm.workingSets.default": "empty",
    "files.associations": {
        "*.sql": "jinja-sql",
        "*.yaml": "jinja-yaml",
        "*.yml": "jinja-yaml",
        // "*.html": "jinja-html"
    },
    "[jinja-sql]": {
        "editor.defaultFormatter": "innoverio.vscode-dbt-power-user"
    },
    "dbt.sqlFmtAdditionalParams": [
        "--line-length",
        "120"
    ],
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".*\\.sql(\\.jinja)?$",
                "notMatch": ".*/dbt/.*",
                "isAsync": false,
                "cmd": "echo 'Running sqlfmt on: ${file}' && /Users/luisaceituno/.local/bin/sqlfmt --line-length 120 \"${file}\""
            }
        ]
    },
    //
    //
    // "indentRainbow.excludedLanguages": [
    //     "sql",
    //     "jinja-sql"
    // ],
    "workbench.productIconTheme": "fluent-icons",
    // "gitblame.inlineMessageEnabled": true,
    "makefile.configureOnOpen": true,
    "dbt.sqlFmtPath": "/Users/luisaceituno/.local/bin/sqlfmt",
    "git.blame.editorDecoration.enabled": true,
    // "notebook.inlineValues": true,
    "macros": {
        "refreshTerminal": [
            "workbench.action.terminal.kill",
            "workbench.action.terminal.new"
        ]
    },
    "window.commandCenter": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "dbt.enableNewLineagePanel": true,
    //
    // Copilot and Chat
    //
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": true
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    //
    // Copilot and Chat - Experimental
    //
    "github.copilot.chat.editor.temporalContext.enabled": true,
    "github.copilot.chat.edits.temporalContext.enabled": true,
    "docker.extension.enableComposeLanguageServer": false,
    "parquet-viewer.format": "csv",
    "chat.mcp.autostart": "newAndOutdated",
}